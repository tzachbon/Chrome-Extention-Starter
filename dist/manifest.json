{
  "name": "Chrome-Extention-Starter",
  "author": "ymdevs",
  "version": "1.0.0",
  "description": "Full chrome extension stater! React,Typescript , MobX, Chrome Listener, Scss with styles, Web Worker, Material UI and many more...!",
  "icons": {
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "alarms",
    "bookmarks",
    "cookies",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": ["js/popup.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Kidsafe",
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [],
  "content_scripts": [
    {
      "js": ["js/eventPage.js", "js/webWorker.js"],
      "matches": ["*://*/*"]
    }
  ]
}
